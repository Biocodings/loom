{
    "pipeline": {
        "files": [
            {
                "path": "words/world_X53J7F.txt"
            },
            {
                "import_from": {
                    "path": "/data/hello.in"
                },
                "path": "words/hello_X53J7F.txt"
            },
            {
                "path": "results/helloworld_X53J7F.txt",
                "save_to": {
                    "path": "/data/helloworld_X53J7F.out"
                }
            }
        ],
        "sessions": [
            {
                "session_resource_set": {
                    "cores": 1,
                    "disk_space": "50k",
                    "memory": "16G"
                }
            }
        ],
        "steps": [
            {
                "application": {
                    "docker_image": "echo"
                },
                "command": "echo  world > words/world_X53J7F.txt",
                "output_file_ids": [
                    "worldfile"
                ],
                "session_id": "helloworldsession",
                "step_resource_set": {
                    "cores": 1,
                    "memory": "1G"
                }
            },
            {
                "application": {
                    "docker_image": "cat"
                },
                "command": "cat words/hello_X53J7F.txt words/world_X53J7F.txt > results/helloworld_X53J7F.txt",
                "input_file_ids": [
                    "worldfile",
                    "hellofile"
                ],
                "output_file_ids": [
                    "helloworldfile"
                ],
                "session_id": "helloworldsession",
                "step_resource_set": {
                    "cores": 1,
                    "memory": "1G"
                }
            }
        ]
    }
}
